    "version": "0.2.0",
    "configurations": [
        {
            "name": "Watch (Debug)",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceRoot}/target/debug/fts_pdbsrc.exe",
            "args": [
                "watch"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}",
            "environment": [],
            "externalConsole": false,
        },
        {
            "name": "Extract One (Debug)",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceRoot}/target/debug/fts_pdbsrc.exe",
            "args": [
                "extract_one",
                "--pdb-uuid",
                "dac8cbc3-fff7-48bc-afcb-2ac6197522a9",
                "--file",
                "CrashTest/CrashTest.cpp",
                "--out",
                "C:/Users/Forrest/AppData/Local/fts_pdbsrc/dac8cbc3-fff7-48bc-afcb-2ac6197522a9/CrashTest/CrashTest.cpp",
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}",
            "environment": [],
            "externalConsole": false,
        },
        {
            "name": "Embed (Debug)",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceRoot}/target/debug/fts_pdbsrc.exe",
            "args": [
                "embed",
                "--pdb",
                "c:/temp/pdb/CrashTest.pdb",
                "--roots",
                "c:/temp/cpp"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}",
            "environment": [],
            "externalConsole": false,
        },
        {
            "name": "Install Service (Debug)",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceRoot}/target/debug/fts_pdbsrc.exe",
            "args": [
                "install_service",
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}",
            "environment": [],
            "externalConsole": false,
        },
    ]
}